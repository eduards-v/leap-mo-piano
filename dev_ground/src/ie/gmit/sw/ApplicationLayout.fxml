<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.layout.*?>

<?import javafx.scene.text.Text?>
<BorderPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="ie.gmit.sw.ApplicationLayout"
            fx:id="layout"
            prefHeight="400.0" prefWidth="600.0" >


        <top>
            <GridPane fx:id="feedCont">
                <Text fx:id="feedBack"
                      style="-fx-alignment: top-center; -fx-control-inner-background: dodgerblue;
                   -fx-font-size: 22px; -fx-text-fill: black; -fx-font-weight: bold;" />

            </GridPane>
        </top>

</BorderPane>
